<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="279.0" prefWidth="362.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.gui.controller.ConnectController">
   <children>
      <VBox alignment="CENTER" prefHeight="0.0" prefWidth="362.0">
         <children>
            <Text fill="#2e2e2e" strokeType="OUTSIDE" strokeWidth="0.0" text="Indtast host">
               <font>
                  <Font name="Segoe UI Semibold" size="12.0" />
               </font>
            </Text>
            <TextField fx:id="hostTextField" maxWidth="200.0" onAction="#connectToServer" promptText="f.eks. localhost" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="0.0" prefWidth="362.0">
         <children>
            <Text fill="#2e2e2e" strokeType="OUTSIDE" strokeWidth="0.0" text="Indtast port">
               <font>
                  <Font name="Segoe UI Semibold" size="12.0" />
               </font>
            </Text>
            <TextField fx:id="portTextField" maxWidth="200.0" onAction="#connectToServer" promptText="f.eks. 3000" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="0.0" prefWidth="362.0">
         <children>
            <Text fill="#2e2e2e" strokeType="OUTSIDE" strokeWidth="0.0" text="Indtast brugernavn">
               <font>
                  <Font name="Segoe UI Semibold" size="12.0" />
               </font>
            </Text>
            <TextField fx:id="usernameTextField" maxWidth="200.0" onAction="#connectToServer" />
         </children>
      </VBox>
      <Button mnemonicParsing="false" onAction="#connectToServer" text="Tilsut til server" />
   </children>
</VBox>
